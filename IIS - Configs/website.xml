<?xml version="1.0" encoding="UTF-8"?>

<appcmd>

    <site name="Default Web Site" serverAutoStart="true">

            <bindings>

                <binding protocol="http" bindingInformation="*:80:" />

            </bindings>

            <limits />

            <logFile directory="C:\inetpub\logs\LogFiles\Default">

                <customFields>

                </customFields>

            </logFile>

            <traceFailedRequestsLogging />

            <hsts />

            <applicationDefaults />

            <virtualDirectoryDefaults />

            <application path="##appName##" applicationPool="##poolName##">

                <virtualDirectoryDefaults />

                <virtualDirectory path="/" physicalPath="##ProjectPath##" />                

            </application>

        </site>

    </SITE>
	
</appcmd>
